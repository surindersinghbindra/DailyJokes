<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.regrex.awesomejokes">

    <application
        android:name="com.regrex.jokesupload.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.regrex.jokesupload.excel.ExcelActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.regrex.jokesupload.MainActivity" />

        <provider
            android:name="com.regrex.jokesupload.excel.ShareContentProvider"
            android:authorities="pro.kondratev.xlsxpoiexample"
            android:exported="true" />
    </application>

</manifest>